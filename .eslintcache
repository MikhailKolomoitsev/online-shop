 => {\r\n    const [device, setdevice] = useState({ info: [] })\r\n\r\n    const {id} = useParams()\r\n    \r\n    useEffect(() => {\r\n        fetchDevice(id).then(data=>setdevice(data))\r\n    }, [])\r\n    return (\r\n        <Container className=\"mt-3\">\r\n            <Row>\r\n\r\n                <Col md={4}>\r\n                    <Image width={300} height={300} src={process.env.REACT_APP_API_URL+device.img} />\r\n                </Col>\r\n                <Col md={4}>\r\n                    <Row className=\"d-flex flex-column align-items-center\">\r\n                        <h2>{device.name}</h2>\r\n                        <div\r\n                            className=\"d-flex align-items-center justify-content-center\"\r\n                            style={{\r\n                                background: `url(${bigStar}) no-repeat center center`,\r\n                                width: 250, height: 240,\r\n                                backgroundSize: \"cover\",\r\n                                fontSize: 64\r\n                            }}\r\n                        >\r\n                            {device.raiting}\r\n                        </div>\r\n                    </Row>\r\n                </Col>\r\n                <Col md={4}>\r\n                    <Card\r\n                        className=\"d-flex flex-columnt align-items-center justify-content-center\"\r\n                        style={{width:300, height: 300, fontSize:32, border: '3px solid lightgrey'}}\r\n                    >\r\n                        <h3>Price from: {device.price} ₴</h3>\r\n                        <Button variant={\"outline-success\"}>Add to chart</Button>\r\n                    </Card>\r\n                </Col>\r\n            </Row>\r\n            <Row className=\"d-flex flex-column m-3\" >\r\n                <h2>Characteristics</h2>\r\n                {device.info.map((item, index) =>\r\n                    <Row key={item.id} style={{background: index%2===0?'lightgrey':'transparent'}}>\r\n                        {item.title}: {item.description}\r\n                    </Row>\r\n                        )}\r\n            </Row>\r\n        </Container>\r\n    )\r\n}\r\n\r\nexport default Device","C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\pages\\Basket.js",[],"C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\pages\\Admin.jsx",[],"C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\store\\UserStore.js",[],"C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\store\\DeviceStore.js",["116"],"import { makeAutoObservable } from \"mobx\"\r\n\r\nexport default class Device {\r\n    constructor() {\r\n        this._types = []\r\n        this._brands = [\r\n        ]\r\n        this._devices = []\r\n        this._selectedType = {}\r\n        this._selectedBrand = {}\r\n        this._page = 1\r\n        this._totalCount = 0\r\n        this._limit=3\r\n        makeAutoObservable(this)\r\n    }\r\n\r\n    setTypes(types) {\r\n        this._types = types\r\n    }\r\n\r\n    setBrands(brands) {\r\n        this._brands = brands\r\n    }\r\n    setDevices(devices) {\r\n        this._devices = devices\r\n    }\r\n    setPage(page) {\r\n        this._page = page\r\n    }\r\n    setSelectedType(type) {\r\n        this.setPage(1)\r\n        this._selectedType = type\r\n    }\r\n    setSelectedBrand(brand) {\r\n        this.setPage(1)\r\n        this._selectedBrand = brand\r\n    }\r\n    setPage(page) {\r\n        this._page = page\r\n    }\r\n    setTotalCount(totalCount) {\r\n        this._totalCount = totalCount\r\n    }\r\n    setLimit(limit) {\r\n        this._limit = limit\r\n    }\r\n\r\n    get isAuth() {\r\n        return this._isAuth\r\n    }\r\n\r\n    get user() {\r\n        return this._brands\r\n    }\r\n\r\n    get brands() {\r\n        return this._brands\r\n    }\r\n    get selectedType() {\r\n        return this._selectedType\r\n    }\r\n    get selectedBrand() {\r\n        return this._selectedBrand\r\n    }\r\n    get page() {\r\n        return this._page\r\n    }\r\n    get totalCount() {\r\n        return this._totalCount\r\n    }\r\n    get limit() {\r\n        return this._limit\r\n    }\r\n}","C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\components\\NavBar.jsx",[],"C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\components\\TypeBar.js",[],"C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\components\\BrandBar.js",["117","118","119"],"import { observer } from 'mobx-react-lite'\r\nimport React, { useContext } from 'react'\r\nimport { Context } from '../index'\r\nimport { Container, Row, Col, Card, ListGroup } from 'react-bootstrap'\r\n\r\nconst BrandBar = observer(() => {\r\n    const { device } = useContext(Context)\r\n    return (\r\n        <Row className='d-flex'>\r\n            {device._brands.map(brand =>\r\n                <Card\r\n                    style={{ cursor: 'pointer' }}\r\n                    key={brand.id}\r\n                    className=\"p-3 mr-3\"\r\n                    onClick={() => device.setSelectedBrand(brand)}\r\n                    border={brand.id === device.selectedBrand.id ? 'primary' : 'light'}\r\n                >\r\n                    {brand.name}\r\n                </Card>\r\n            )}\r\n        </Row>\r\n    )\r\n})\r\n\r\nexport default BrandBar\r\n","C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\components\\DeviceList.js",[],"C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\components\\DeviceItem.js",[],"C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\components\\modals\\CreateType.js",[],"C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\components\\modals\\CreateBrand.js",[],"C:\\Users\\a4tec\\OneDrive\\Desktop\\React\\online-shop\\client\\src\\components\\modals\\CreateDevice.js",["120","121"],"import React, { useEffect } from 'react' //rafce\r\nimport { createDevice, fetchBrands, fetchDevices, fetchTypes } from '../../http/deviceAPI'\r\nimport { useState } from 'react'\r\nimport { useContext } from 'react'\r\nimport { Modal, Button, Form, Dropdown, Row, Col } from 'react-bootstrap'\r\nimport { Context } from '../..'\r\nimport { observer } from 'mobx-react-lite'\r\n\r\nconst CreateDevice = observer( ({ show, onHide }) => {\r\n    const { device } = useContext(Context)\r\n\r\n    const [name, setname] = useState('')\r\n    const [price, setprice] = useState(0)\r\n    const [file, setfile] = useState(null)\r\n    const [info, setInfo] = useState([])\r\n\r\n    const addInfo = () => {\r\n        setInfo([...info, { title: '', description: '', number: Date.now() }])\r\n    }\r\n    const deleteInfo = (number) => {\r\n        setInfo(info.filter(i => i.number !== number))\r\n    }\r\n    const selectFile = e => {\r\n        setfile(e.target.files[0])\r\n    }\r\n    const changeInfo = (key, value, number) => {\r\n        setInfo(info.map(i => i.number === number ? { ...i, [key]: value } : i))\r\n    }\r\n    const addDevice = () => {\r\n        const formData = new FormData() //Form data because we upload file\r\n        formData.append('name', name)\r\n        formData.append('price', `${price}`)\r\n        formData.append('img', file)\r\n        formData.append('brandId', device.selectedBrand.id)\r\n        formData.append('typeId', device.selectedType.id)\r\n        formData.append('info', JSON.stringify(info))\r\n        createDevice(formData).then(data => onHide())\r\n    }\r\n\r\n    useEffect(() => {\r\n        fetchTypes().then(data => device.setTypes(data))\r\n        fetchBrands().then(data => device.setBrands(data))\r\n    }, [])\r\n\r\n    return (\r\n        <Modal\r\n            show={show}\r\n            onHide={onHide}\r\n            size=\"lg\"\r\n            centered\r\n        >\r\n            <Modal.Header closeButton>\r\n                <Modal.Title id=\"contained-modal-title-vcenter\">\r\n                    Add the New Device\r\n                </Modal.Title>\r\n            </Modal.Header>\r\n            <Modal.Body>\r\n                <Form className=''>\r\n                    <Dropdown className='mt-3'>\r\n                        <Dropdown.Toggle\r\n                            className='mr-2'>\r\n                            {device.selectedType.name || \"Choose type\"}\r\n                        </Dropdown.Toggle>\r\n                        <Dropdown.Menu>\r\n                            {device._types.map(type =>\r\n                                <Dropdown.Item\r\n                                    onClick={() => device.setSelectedType(type)}\r\n                                    key={type.id}>\r\n                                    {type.name}\r\n                                </Dropdown.Item>)}\r\n                        </Dropdown.Menu>\r\n                    </Dropdown>\r\n                    <Dropdown className='mt-3'>\r\n                        <Dropdown.Toggle\r\n                            className='mr-2'>{device.selectedBrand.name || \"Choose brand\"}\r\n                        </Dropdown.Toggle>\r\n                        <Dropdown.Menu>\r\n                            {device._brands.map(brand =>\r\n                                <Dropdown.Item\r\n                                    onClick={() => device.setSelectedBrand(brand)}\r\n                                    key={brand.id}>\r\n                                    {brand.name}\r\n                                </Dropdown.Item>)}\r\n                        </Dropdown.Menu>\r\n                    </Dropdown>\r\n                    <Form.Control\r\n                        value={name}\r\n                        onChange={e => setname(e.target.value)}\r\n                        className='mt-3'\r\n                        placeholder='Insert name'\r\n                    />\r\n                    <Form.Control\r\n                        value={price}\r\n                        onChange={e => setprice(Number(e.target.value))}\r\n                        className='mt-3'\r\n                        placeholder='Insert price'\r\n                        type='number'\r\n                    />\r\n                    <Form.Control\r\n                        className='mt-3'\r\n                        placeholder='Upload picture'\r\n                        type='file'\r\n                        onChange={selectFile}\r\n                    />\r\n                    <hr></hr>\r\n                    <Button\r\n                        variant={'outline-dark'}\r\n                        onClick={addInfo}\r\n                    >\r\n                        Add new property\r\n                    </Button>\r\n                    {info.map(i =>\r\n                        <Row className='mt-3' key={i.number}>\r\n                            <Col md={4}>\r\n                                <Form.Control\r\n                                    value={i.title}\r\n                                    onChange={(e)=>changeInfo(\"title\", e.target.value, i.number)}\r\n                                    placeholder='Name of the property'\r\n                                />\r\n                            </Col>\r\n                            <Col md={4}>\r\n                                <Form.Control\r\n                                    value={i.description}\r\n                                    onChange={(e)=>changeInfo(\"description\", e.target.value, i.number)}\r\n                                    placeholder='Desc of the property'\r\n                                />\r\n                            </Col>\r\n                            <Col md={4}>\r\n                                <Button\r\n                                    variant={'outline-danger'}\r\n                                    onClick={() => deleteInfo(i.number)}\r\n                                >\r\n                                    Delete\r\n  